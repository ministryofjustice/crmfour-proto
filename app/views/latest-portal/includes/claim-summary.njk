
  {% if data['paymentType'] =="Assigned counsel" or data['paymentType'] =="Assigned counsel - appeal" or data['paymentType'] =="Assigned counsel - amendment" %}
      <div class="govuk-summary-list__row">
        <dt class="govuk-summary-list__key">
          Claim type
        </dt>
        <dd class="govuk-summary-list__value">
          {{data['paymentType']}}
        </dd>
      </div>
      <div class="govuk-summary-list__row">
        <dt class="govuk-summary-list__key">
          Linked non-standard claim
        </dt>
        <dd class="govuk-summary-list__value">
          {% if data['linkedCRM'] =="Yes"%}
            {{data['laa-ref'] or "LAA-1z5f76"}}
          {% else %}
            Not linked to a non-standard magistrates' claim
          {% endif %}
        </dd>
      </div>
    {% if data['paymentType'] =="Assigned counsel - appeal" or data['paymentType'] =="Assigned counsel - amendment" %}
      <div class="govuk-summary-list__row">
        <dt class="govuk-summary-list__key">
          Linked assigned counsel claim
        </dt>
        <dd class="govuk-summary-list__value">
          {% if data['linkedCRM'] =="Yes"%}
            {{data['laa-ref'] or "LAA-5a7d3c"}}
          {% else %}
            Not linked to an assigned counsel claim
          {% endif %}
        </dd>
      </div>
    {% endif %}
      <div class="govuk-summary-list__row">
        <dt class="govuk-summary-list__key">
          {% if data['paymentType'] =="Assigned counsel" %}
            Date claim received
          {% elseif data['paymentType'] =="Assigned counsel - appeal" %}
            Date appeal received
          {% elseif data['paymentType'] =="Assigned counsel - amendment" %}
            Date claim amended
          {% endif %}
        </dt>
        <dd class="govuk-summary-list__value">
          {{data['dateReceived'] or "24 July 2025"}}
        </dd>
      </div>
      <div class="govuk-summary-list__row">
        <dt class="govuk-summary-list__key">
